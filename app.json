{
  "expo": {
    "name": "disposable art",
    "slug": "disposable art",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "assetBundlePatterns": ["assets/images/*", "assets/fonts/*"],
    "icon": "./assets/images/favicon.png",
    "web": {
      "bundler": "metro",
      "build": {
        "output": "docs"
      },
      "favicon": "./assets/images/favicon.png",
      "basePath": "./"
    },
    "plugins": ["expo-router"]
  }
}
