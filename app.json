{
  "expo": {
    "name": "DailyTask",
    "slug": "DailyTask",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "platforms": ["ios", "android", "web"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "#89CC04"
      }
    },
    "assetBundlePatterns": ["assets/images/*", "assets/fonts/*"],
    "web": {
      "bundler": "metro",
      "build": {
        "output": "static"
      },
      "favicon": "./assets/images/favicon.png",
      "basePath": ""
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c7372570-1f26-4751-9224-64b177ca7de0"
      }
    }
  }
}
